  "main": "index.js",
  "scripts": {
    "test": "jest --env=jsdom",
    "testJest": "jest src/tests/sum --notify --config=jest.config.js",
    "testP": " node ./src/tests/puppeteer/example.js",
    "gql": "node ./src/gql/server.js",
    "postinstall": "npm start",
    "runserver": "(cd ./src &&  nodemon  ./server)",
    "prestart": "(cd ./src && start nodemon  ./server)  ",
    "start": " webpack serve   --hot --env PLATFORM=local --env VERSION=stag --mode development --config config/webpack.base.config.js --history-api-fallback --open Chrome.exe",
